# -*- mode: conf -*-
root = true

[*]
end_of_line = lf
insert_final_newline = true

[*.{coffee,less,js,jsx}]
indent_style = space
indent_size = 2

[*.html]
indent_style = space
indent_size = 2

[*.c]
indent_style = space
indent_size = 2

[*.java]
indent_style = space
indent_size = 2

[Makefile]
indent_style = tab
